{
  "name": "typescript-restful-starter",
  "version": "0.1.0",
  "description": "kit starter node application",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && cd dist && cross-env NODE_ENV=PRODUCTION node Index.js",
    "dev": "nodemon",
    "test": "cross-env NODE_ENV=test nyc --reporter=html mocha test/**/*.ts",
    "docker-test": "bash scripts/test.sh",
    "build": "npm run clean && tsc --outDir dist",
    "lint": "tslint app/**/*.ts",
    "clean": "rimraf dist",
    "serve-docs": "apidoc -i ./app -o ./docs && http-server docs",
    "seed:users": "node -r ts-node/register ./cli/Seeder.ts",
    "test-user": "node -r ts-node/register ./cli/GetTestUser.ts",
    "change-role": "node -r ts-node/register ./cli/ChangeRole.ts"
  },
  "nyc": {
    "include": [
      "app/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "typescript",
    "tslint",
    "JWT",
    "E2015"
  ],
  "author": "Hector Riquelme",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/faker": "^4.1.4",
    "aws-sdk": "^2.355.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.2",
    "class-validator": "^0.8.5",
    "cluster": "^0.7.7",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.11",
    "mailgun.js": "^2.0.1",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "mysql": "^2.15.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "superagent": "^3.8.2",
    "token-extractor": "^0.1.6",
    "ts-node": "^7.0.0",
    "typeorm": "^0.2.8",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^2.0.0",
    "@types/chai": "^4.1.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "0.0.40",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/lodash": "^4.14.116",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^5.2.4",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.5.2",
    "@types/rimraf": "2.0.2",
    "@types/superagent": "^3.5.7",
    "@types/supertest": "^2.0.4",
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "http-server": "^0.11.1",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.2",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.2",
    "supertest": "^3.1.0",
    "tslint": "^5.9.1"
  }
}
